			<ImageView id="image" image="/AIH_Logo.png" />
			<View id='health'>
				<ImageView id="healthImage" image="/YellowArrow.png" />
			</View>
			<Label id='over'>You Are 79% healthy</Label>
			<View id='nav'>
				<Button class='rowOne' id='chart' />
				<Button class='rowOne' id='doc' />
				<Button class='rowOne' id='sick' />
